			<!-- Account Signup --> 
			<!-- Request -->
			<xsd:complexType name="accountSignupRequest"> 
				<xsd:sequence> 
					<xsd:element name="email" type="xsd:string" />
					<xsd:element name="password" type="xsd:string" />
					<xsd:element name="name" type="xsd:string" />
					<xsd:element name="surname" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>
				
			<!-- Response -->
			<xsd:complexType name="accountSignupResponse">
				<xsd:sequence>
					<xsd:element name="token" type="xsd:string" />
					<xsd:element name="id" type="xsd:long" />
					<xsd:element name="message" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>
			
			<!-- Fault-->
			<xsd:complexType name="accountSignupFault">
				<xsd:sequence>
		        	<xsd:element name="faultInsert" type="xsd:string" />
		            <xsd:element name="message" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>
			
			<!-- Account Login -->
			<!-- Request -->
			<xsd:complexType name="accountLoginRequest">
				<xsd:sequence>
					<xsd:element name="email" type="xsd:string" />
					<xsd:element name="password" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>
			
			<!-- Response -->
			<xsd:complexType name="accountLoginResponse">
				<xsd:sequence>
					<xsd:element name="token" type="xsd:string" />
					<xsd:element name="id" type="xsd:long" />
				</xsd:sequence>
			</xsd:complexType>
			
			<!-- Fault-->
			<xsd:complexType name="accountLoginFault">
