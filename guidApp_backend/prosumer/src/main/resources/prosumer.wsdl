				</xsd:sequence>
			</xsd:complexType>
					
			<!-- Fault -->
			<xsd:complexType name="attractionByCreatorResearchFault">
				<xsd:sequence>
                        <xsd:element name="faultInfo" type="xsd:string" />
                        <xsd:element name="message" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>
			
			
			<!-- Research Event Detail -->
			<!-- Request -->
			<xsd:complexType name="eventDetailResearchRequest">
				<xsd:sequence>
					<xsd:element name="id" type="xsd:long" />
				</xsd:sequence>
			</xsd:complexType>
			
			<!-- Response -->
			<xsd:complexType name="eventDetailResearchResponse">
			      <xsd:sequence>
			      	<xsd:element name="eventElement" type="tns:eventElement" />	
			      	<xsd:element name="message" type="xsd:string" />	
			      </xsd:sequence>
			</xsd:complexType>
					
			<!-- Fault -->
			<xsd:complexType name="eventDetailResearchFault">
				<xsd:sequence>
                        <xsd:element name="faultInfo" type="xsd:string" />
                        <xsd:element name="message" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>
			
			
