					<xsd:element name="attractionElement" maxOccurs="unbounded" type="tns:attractionElement" />
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="attractionElement">
			      <xsd:sequence>
			      	<xsd:element name="id" type="xsd:long" />
					<xsd:element name="name" type="xsd:string" />
					<xsd:element name="locality" type="xsd:string" />
					<xsd:element name="categoryName" type="xsd:string" />
					<xsd:element name="categoryId" type="xsd:long" />
					<xsd:element name="creatorId" type="xsd:long" />	
					<xsd:element name="lat" type="xsd:string" />
					<xsd:element name="lng" type="xsd:string" />
			      </xsd:sequence>
			</xsd:complexType>
					
			<xsd:complexType name="eventsList" type="tns:eventsList">
				<xsd:sequence>
					<xsd:element name="eventElement" maxOccurs="unbounded" type="tns:eventElement"/>
				</xsd:sequence>
			</xsd:complexType>
			 
			<xsd:complexType name="eventElement">
			      <xsd:sequence>
			      	<xsd:element name="id" type="xsd:long" />
					<xsd:element name="title" type="xsd:string" />
					<xsd:element name="locality" type="xsd:string" />
					<xsd:element name="startDate" type="xsd:dateTime" />
					<xsd:element name="endDate" type="xsd:dateTime" />
					<xsd:element name="categoryName" type="xsd:string" />
					<xsd:element name="categoryId" type="xsd:long" />
					<xsd:element name="creatorId" type="xsd:long" />	
