					<xsd:element name="locality" type="xsd:string" />
					<xsd:element name="startDate" type="xsd:dateTime" />
					<xsd:element name="endDate" type="xsd:dateTime" />
					<xsd:element name="categoryName" type="xsd:string" />
					<xsd:element name="categoryId" type="xsd:long" />
					<xsd:element name="creatorId" type="xsd:long" />
					<xsd:element name="lat" type="xsd:string" />
					<xsd:element name="lng" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>
			
			<!-- Response -->
			<xsd:complexType name="eventUpdateResponse">
				<xsd:sequence>
					<xsd:element name="message" type="xsd:string" />					
				</xsd:sequence>
			</xsd:complexType>
					
			<!-- Fault -->
			<xsd:complexType name="eventUpdateFault">
				<xsd:sequence>
	            	<xsd:element name="faultInfo" type="xsd:string" />
	                <xsd:element name="message" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>
		   
			<!-- Insert Attraction -->
			<!-- Request -->
			<xsd:complexType name="attractionInsertRequest">
				<xsd:sequence>
					<xsd:element name="name" type="xsd:string" />
					<xsd:element name="locality" type="xsd:string" />
					<xsd:element name="categoryName" type="xsd:string" />
					<xsd:element name="categoryId" type="xsd:long" />
					<xsd:element name="creatorId" type="xsd:long" />
					<xsd:element name="lat" type="xsd:string" />
					<xsd:element name="lng" type="xsd:string" />
				</xsd:sequence>
