			<xsd:complexType name="eventResearchRequest">
				<xsd:sequence>
					<xsd:element name="title" type="xsd:string" minOccurs="0" maxOccurs="1"  />
					<xsd:element name="locality" type="xsd:string" minOccurs="0" maxOccurs="1" />
					<xsd:element name="date" type="xsd:dateTime" minOccurs="0" maxOccurs="1" />
					<xsd:element name="categoryId" type="xsd:long" minOccurs="0" maxOccurs="1" />
				</xsd:sequence>
			</xsd:complexType>
			
			<!-- Response -->
			<xsd:complexType name="eventResearchResponse">
				<xsd:sequence>
					<xsd:element name="message" type="xsd:string" />	
					<xsd:element name="eventsList" type="tns:eventsList" />										
				</xsd:sequence>
			</xsd:complexType>
					
			<!-- Fault -->
			<xsd:complexType name="eventResearchFault">
				<xsd:sequence>
                        <xsd:element name="faultInfo" type="xsd:string" />
                        <xsd:element name="message" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>
			
			
			<!-- Research Attraction by Filter -->
			<!-- Request -->
			<xsd:complexType name="attractionResearchRequest">
				<xsd:sequence>
					<xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="1" />
					<xsd:element name="locality" type="xsd:string" minOccurs="0" maxOccurs="1" />
					<xsd:element name="categoryId" type="xsd:long" minOccurs="0" maxOccurs="1" />
				</xsd:sequence>
			</xsd:complexType>
			
			<!-- Response -->
			<xsd:complexType name="attractionResearchResponse">
				<xsd:sequence>
					<xsd:element name="message" type="xsd:string" />	
					<xsd:element name="attractionsList" type="tns:attractionsList" />										
				</xsd:sequence>
			</xsd:complexType>
					
			<!-- Fault -->
			<xsd:complexType name="attractionResearchFault">
				<xsd:sequence>
                        <xsd:element name="faultInfo" type="xsd:string" />
                        <xsd:element name="message" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>
			
