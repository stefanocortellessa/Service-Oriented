<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xmlns:jaxws="http://cxf.apache.org/jaxws" 
       xmlns:cxf="http://cxf.apache.org/core" 
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd 
                           http://cxf.apache.org/core http://cxf.apache.org/schemas/core.xsd 
                           http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">
                           
    <jaxws:endpoint id="AccountManagerHTTP" 
			xmlns:accountmanager="http://it.univaq.disim.sose/accountmanager"
		    address="/accountmanager" 
		    serviceName="accountmanager:AccountManagerService"
		    endpointName="accountmanager:accountManagerPort" 
		    implementor="#AccountManagerPTImpl">
	</jaxws:endpoint>
	
</beans>